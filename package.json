{
  "name": "loggerect",
  "version": "1.0.1",
  "description": "A powerful, highly configurable React logger with TypeScript decorators, environment-aware logging, and source path tracking",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./core": {
      "import": "./dist/core.esm.js",
      "require": "./dist/core.js",
      "types": "./dist/core.d.ts"
    },
    "./react": {
      "import": "./dist/react.esm.js",
      "require": "./dist/react.js",
      "types": "./dist/react.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks.esm.js",
      "require": "./dist/hooks.js",
      "types": "./dist/hooks.d.ts"
    },
    "./babel-plugin": {
      "require": "./babel-plugin/index.js",
      "default": "./babel-plugin/index.js"
    },
    "./vite-plugin": {
      "require": "./vite-plugin/index.js",
      "default": "./vite-plugin/index.js"
    },
    "./unplugin": {
      "require": "./unplugin/index.js",
      "default": "./unplugin/index.js"
    },
    "./loader": {
      "require": "./loader/index.js",
      "default": "./loader/index.js"
    }
  },
  "files": [
    "dist",
    "babel-plugin",
    "vite-plugin",
    "unplugin",
    "loader",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c && rm -f dist/*.map",
    "dev": "rollup -c -w",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "keywords": [
    "react",
    "logger",
    "logging",
    "typescript",
    "decorator",
    "debug",
    "console",
    "development",
    "production",
    "trace",
    "stack-trace",
    "source-map",
    "vite",
    "webpack",
    "turbopack",
    "babel",
    "unplugin"
  ],
  "author": "Md. Fahad-Uz-Zaman Khan <fahadtanim@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "@babel/core": ">=7.0.0",
    "react": ">=16.8.0",
    "vite": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "@babel/core": {
      "optional": true
    },
    "vite": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/babel__core": "^7.20.5",
    "@types/node": "^25.0.3",
    "@types/react": "^18.2.48",
    "magic-string": "^0.30.21",
    "react": "^18.2.0",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "tslib": "^2.8.1",
    "typescript": "^5.3.3",
    "unplugin": "^2.3.11",
    "vitest": "^1.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fahadtanim/loggerect"
  },
  "bugs": {
    "url": "https://github.com/fahadtanim/loggerect/issues"
  },
  "homepage": "https://fahadtanim.github.io/loggerect"
}
